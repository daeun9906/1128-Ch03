<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
	<meta charset="UTF-8">
	
	<!-- JScript 연습용  -->
 	<script type="text/javascript">
 		<!-- 함수 선언 -->
 		function logincheck(){
 			alert("자바스키립트가 실행됨!\n logincheck 함수 실행");
 			
 			
 		}
 	
 	
 	</script>
 	
 	
	<title>Insert title here</title>
</head>
<body>

	<form action="">
		<!-- onclick: 작동할 함수를 괄호까지 넣기-->
		<input type = "submit" value="자바스크립트" onclick = "logincheck()">
	
	
	</form>




</body>
</html>